{
  "manifest_version": 3,
  "name": "TradingView Symbol Switcher",
  "version": "1.1",
  "description": "Manage and switch between additional stock symbols.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
